def sum_of_difference(list1, list2):
    diff_sum = 0
    for i in range(len(list1)):
        if list1[i]-list2[i]<0:
            diff_sum += (list1[i]-list2[i])*(-1)
        else:
            diff_sum += (list1[i]-list2[i])
    
    return diff_sum


list1 = [5,12,5,6,2,6,7,8]

list2 = [6,4,3,8,1,2,4,6]
sum_of_difference(list1, list2)
